{
	"env": {
		"node": true,
		"browser": true,
		"commonjs": true,
		"es2021": true
	},
	"extends": "comfycase",
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"@typescript-eslint"
	],
	"rules": {
		"no-unused-vars": "off",
		"@typescript-eslint/no-unused-vars": [
			"warn"
		]
	}
}
